{
    "name": "blog",
    "version": "0.1.0",
    "description": "My blog built with Jekyll and hosted with GitHub Pages.",
    "repository": {
        "type": "git",
        "url": "git://github.com/xransum/blog.git"
    },
    "keywords": [
        "jekyll"
    ],
    "author": "Kevin Haas",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xransum/blog/issues"
    },
    "homepage": "https://xransum.github.io/blog/",
    "dependencies": {
        "autoprefixer": "^9.8.2",
        "bootstrap": "^4.1.3",
        "dom101": "^2.2.1",
        "fontawesome-free": "^1.0.4",
        "highlight.js": "^11.8.0",
        "hint.css": "^2.6.0",
        "isotope-layout": "^3.0.6",
        "jquery": "^3.7.0",
        "jquery-ui-dist": "^1.13.2",
        "lodash.noop": "^3.0.1",
        "modularscale-sass": "^3.0.10",
        "onmount": "^1.3.0",
        "popper.js": "^1.16.1",
        "postcss-modules": "^2.0.0",
        "prismjs": "^1.20.0",
        "sanitize.css": "^11.0.1",
        "sass": "^1.63.4"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@rstacruz/prettier-plugin-markdown-code-fences": "^1.0.0",
        "jest": "26.0.1",
        "jest-html": "1.5.0",
        "netlify-plugin-minify-html": "^0.2.3",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.0.5",
        "wait-on": "^5.0.1"
    },
    "jest": {
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-html"
        ],
        "collectCoverageFrom": [
            "**/*.js"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/packages/*/node_modules/",
            "<rootDir>/examples/",
            "<rootDir>/coverage/"
        ]
    },
    "main": "index.js",
    "private": true,
    "scripts": {
        "build": "run-s -s 'deps:build' jekyll:build",
        "deps:build": "./scripts/build_deps",
        "dev": "run-p -sl 'deps:build' jekyll:watch",
        "jekyll:build": "bundle exec jekyll build --verbose",
        "jekyll:watch": "bundle exec jekyll serve --trace --drafts --watch --incremental --host=0.0.0.0 --port=4000",
        "prejekyll:build": "bundle",
        "prejekyll:watch": "bundle",
        "prettier:format": "prettier --write '_parcel/**/*.{js,scss}'",
        "test": "jest"
    },
    "volta": {
        "node": "16.14.2",
        "yarn": "1.22.18"
    }
}

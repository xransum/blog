  "homepage": "https://github.com/xransum/blog#readme",
  "dependencies": {
    "bootstrap": "^4.6.2",
    "bootstrap-icons": "^1.10.5",
    "clipboard.js": "^1.0.1",
    "fontawesome-free": "^1.0.4",
    "graphviz": "^0.0.9",
    "highlight.js": "^11.8.0",
    "jquery": "^3.7.0",
    "jquery-ui-dist": "^1.13.2",
    "popper.js": "^1.16.1"
  },
  "scripts": {
    "init": "sh scripts/bootstrap",
    "serve": "bundle exec jekyll serve --safe",
    "serve:nobuild": "npm-run-all serve --skip-initial-build",
    "serve:watch": "npm run serve -- --watch --livereload --livereload-min-delay 5 --drafts --unpublished --future --strict_front_matter --trace --incremental --config _config.yml,_config-local.yml",
    "build": "bundle exec jekyll build --future --strict_front_matter --trace --config _config.yml,_config-local.yml",
    "dev": "npm-run-all serve:watch",
    "test": "echo \"No tests yet...\" && exit 0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}

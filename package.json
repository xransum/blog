{
    "name": "blog",
    "version": "0.1.0",
    "description": "My blog built with Jekyll and hosted with GitHub Pages.",
    "repository": {
        "type": "git",
        "url": "git://github.com/xransum/blog.git"
    },
    "keywords": [
        "jekyll"
    ],
    "author": "Kevin Haas",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/xransum/blog/issues"
    },
    "homepage": "https://xransum.github.io/blog/",
    "dependencies": {
        "autoprefixer": "^9.8.2",
        "bootstrap": "5.3",
        "fontawesome-free": "^1.0.4",
        "highlight.js": "^11.8.0",
        "jquery": "^3.7.0",
        "jquery-ui-dist": "^1.13.2",
        "popper.js": "^1.16.1"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "jest": "26.0.1",
        "jest-html": "1.5.0",
        "netlify-plugin-minify-html": "^0.2.3",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "wait-on": "^5.0.1"
    },
    "jest": {
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-html"
        ],
        "collectCoverageFrom": [
            "**/*.js"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/packages/*/node_modules/",
            "<rootDir>/examples/",
            "<rootDir>/coverage/"
        ]
    },
    "main": "index.js",
    "private": true,
    "scripts": {
        "build": "script/build",
        "dev": "run-s -s 'build' jekyll:watch",
        "dev:nobuild": "run-s -s jekyll:watch",
        "jekyll:watch": "bundle exec jekyll serve --safe --trace --drafts --watch --incremental",
        "prettier:format": "prettier --write '_parcel/**/*.{js,scss}'",
        "test": "./script/test"
    },
    "volta": {
        "node": "16.14.2",
        "yarn": "1.22.18"
    }
}
